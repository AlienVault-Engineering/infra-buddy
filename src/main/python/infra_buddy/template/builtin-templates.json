{
  "service-modification-templates": {
    "autoscale": {
      "type": "github",
      "compatible": [
        "default-api-service",
        "api-service",
        "batch-service",
        "ecs-service"
      ],
      "owner": "AlienVault-Engineering",
      "repo": "infra-buddy-ecs-service-autoscale"
    },
    "cluster-cpu-autoscale": {
      "type": "github",
      "compatible": [
        "cluster"
      ],
      "owner": "AlienVault-Engineering",
      "repo": "infra-buddy-cluster-cpu-autoscale"
    },
    "cluster-memory-autoscale": {
      "type": "github",
      "compatible": [
        "cluster"
      ],
      "owner": "AlienVault-Engineering",
      "repo": "infra-buddy-cluster-memory-autoscale"
    },
    "sqs-threshold": {
      "type": "github",
      "compatible": [
        "default-api-service",
        "api-service",
        "batch-service",
        "ecs-service"
      ],
      "owner": "AlienVault-Engineering",
      "repo": "infra-buddy-ecs-service-sqs-threshold"
    },
    "target-connection-error": {
      "type": "github",
      "compatible": [
        "default-api-service",
        "api-service",
        "batch-service",
        "ecs-service"
      ],
      "owner": "AlienVault-Engineering",
      "repo": "infra-buddy-ecs-service-target-connection-error"
    },
    "rds-aurora": {
      "type": "github",
      "compatible": [
        "*"
      ],
      "owner": "AlienVault-Engineering",
      "repo": "infra-buddy-rds-aurora"
    }
  },
  "service-templates": {
    "cluster": {
      "type": "github",
      "owner": "AlienVault-Engineering",
      "repo": "infra-buddy-cluster"
    },
    "vpc": {
      "type": "github",
      "owner": "AlienVault-Engineering",
      "repo": "infra-buddy-vpc"
    },
    "ecs-service": {
      "type": "github",
      "owner": "AlienVault-Engineering",
      "repo": "infra-buddy-ecs-service"
    },
    "default-api-service": {
      "type": "alias",
      "lookup": "ecs-service",
      "default-values": {
        "DEFAULT_LOAD_BALANCER_TARGET": true
      }
    },
    "api-service": {
      "type": "alias",
      "lookup": "ecs-service",
      "default-values": {
        "CREATE_API": true
      }
    },
    "batch-service": {
      "type": "alias",
      "lookup": "ecs-service",
      "default-values": {
        "CREATE_API": false,
        "DEFAULT_LOAD_BALANCER_TARGET": false
      }
    },
    "listener": {
      "type": "alias",
      "lookup": "ecs-service",
      "default-values": {
        "CREATE_API": false,
        "DEFAULT_LOAD_BALANCER_TARGET": false
      }
    },
    "rds-aurora": {
      "type": "github",
      "owner": "AlienVault-Engineering",
      "repo": "infra-buddy-rds-aurora"
    },
    "cloudfront-angular": {
      "type": "github",
      "owner": "AlienVault-Engineering",
      "repo": "infra-buddy-cloudfront-angular"
    }
  }
}